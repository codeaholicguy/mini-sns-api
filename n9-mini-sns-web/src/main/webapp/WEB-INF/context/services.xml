<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:context="http://www.springframework.org/schema/context"
	xsi:schemaLocation="http://www.springframework.org/schema/beans 
						http://www.springframework.org/schema/beans/spring-beans-4.0.xsd
						http://www.springframework.org/schema/context 
						http://www.springframework.org/schema/context/spring-context-4.0.xsd">

	<context:component-scan base-package="com.n9.mini.sns.services" />

	<bean name="fileProcessingServices" class="com.n9.mini.sns.services.FileProcessingServicesImpl" />

	<bean name="accountServices" class="com.n9.mini.sns.services.AccountServicesImpl" />
	<bean name="imageServices" class="com.n9.mini.sns.services.ImageServicesImpl" />
	<bean name="tagServices" class="com.n9.mini.sns.services.TagServicesImpl" />
	<bean name="feedServices" class="com.n9.mini.sns.services.FeedServicesImpl" />
	<bean name="commentServices" class="com.n9.mini.sns.services.CommentServicesImpl" />

</beans>
